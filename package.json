{
  "name": "geppetto",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "dotenv -- turbo dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{json,js,ts,tsx,md}\""
  },
  "devDependencies": {
    "dotenv-cli": "^7.1.0",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@9.6.2"
}
